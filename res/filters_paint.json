[
  {
    "name": "exception",
    "formats": [
      {
        "displays": [
          {
            "pattern": "- fatal : {}",
            "path": "log.message"
          }
        ],
        "path": "log.type",
        "value": "exception",
        "style": [
          "red",
          "bold"
        ]
      }
    ]
  },
  {
    "name": "actions_project_init",
    "formats": [
      {
        "displays": [
          {
            "pattern": "- result : {}",
            "path": "log.result.status"
          }
        ],
        "path": "log.result.status",
        "value": "change",
        "style": [
          "yellow",
          "bold"
        ]
      },
      {
        "displays": [
          {
            "pattern": "- ??? : {} : ",
            "path": "question"
          }
        ],
        "path": "question",
        "style": [
          "red",
          "bold"
        ]
      },
      {
        "path": "title.value",
        "displays": [
          {
            "pattern": "######################\n####  {}\n######################",
            "path": "title.value"
          }
        ],
        "style": [
          "yellow",
          "bold"
        ]
      }
    ]
  }
]
